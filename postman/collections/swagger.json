        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "The name of the item",
            "example": "Widget"
          },
          "description": {
            "type": "string",
            "description": "The description of the item",
            "example": "A useful widget for everyday tasks"
          },
          "price": {
            "type": "number",
            "description": "The price of the item in dollars",
            "example": 19.99
          },
          "quantity": {
            "type": "number",
            "description": "The quantity available in stock",
            "example": 100
          }
        },
        "required": [
          "name",
          "description",
          "price",
          "quantity"
